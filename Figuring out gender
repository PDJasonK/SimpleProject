package SocialNumber;

import java.util.Scanner;

public class ssn {

	public static void main(String[] args) {
		System.out.print("Type your social security numbers/(Ex: 901019-2126545");
		
		Scanner sc = new Scanner(System.in);
		String regNo = sc.nextLine();
		
		char gender = regNo.charAt(7);
		
		switch (gender) {
		
		case '1' : case '3':
			System.out.println("Beloning human being is Male");
			break;
		
		case '2' : case '4':
			System.out.println("Belonging human being is Female");
			break;
			
			default :
				System.out.println("This socail security numbers are not avaiable");
		
		}

	}

}
